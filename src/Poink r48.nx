TOUCHSCREEN
PLAYERCOUNT=1

ENDGAME:
GOTO TITLESCREEN

NEWGAME:
COPY ROM(1),32 TO $FF00
CLS
ON VBL OFF
RANDOMIZE TIMER
SPRITE.A 0,1
'CLEAR BALL TRACES
FOR I=0 TO TRACENUM-1
SPRITE.A 7+I,1
NEXT I
TRACENEXT=-1
'CLEAR THE BRICK TO CLEAR TABLE, NORMALLY NOT NEEDED.
FOR I=0 TO 3
TOCLEAR(I,0)=-1
NEXT I
'RANDOMIZE LIST OF LEVEL ID
FOR I=0 TO 31
LEVELIDS(I)=-1
NEXT I
'GET NUMBER OF LEVELS
C=PEEK(ROM(4))
FOR I=1 TO C
_PICKALEVELID:
J=INT(RND*C)
'CHECK IF THE LEVEL HAS BEEN PICKED ALREADY
P=0
FOR K=0 TO 31
IF LEVELIDS(K)=J THEN P=-1
NEXT K
'PICK AND OTHER LEVEL ID
IF P THEN GOTO _PICKALEVELID
'PLACE THE PICKED ID
LEVELIDS(I-1)=J
NEXT I
'CLEAR PLAYERS
FOR I=0 TO PLAYERCOUNT-1
PLAYERS(I,0)=4
'IF FORCELIVE>=0 THEN PLAYERS(I,0)=FORCELIVE
PLAYERS(I,1)=0
PLAYERS(I,2)=0
PLAYERS(I,3)=0
PLAYERS(I,4)=0
NEXT I
CURRPLAYER=0
CURRACT=0
'IF FORCECURRSTAGE>=0 THEN CURRSTAGE=FORCECURRSTAGE
GOSUB RESETPLAY

FOR CURRPLAYER=0 TO PLAYERCOUNT-1
GOSUB FILLLEVELS
NEXT CURRPLAYER
CURRPLAYER=0
GOTO STARTPLAY

FILLLEVELS:
FOR LI=0 TO 3
CURRLEVEL=LEVELIDS(CURRACT*4+LI)
IF CURRLEVEL=-1 THEN GOTO WINGAME
PLAYERSTAGE=4*CURRPLAYER+LI
GOSUB RESETLEVEL
GOSUB LOADLEVEL
NEXT LI
PLAYERSTAGE=4*CURRPLAYER+CURRSTAGE
RETURN

STARTPLAY:
RANDOMIZE TIMER
IF PLAYERCOUNT>1 THEN GOSUB PLAYERTURN
TICK=0
CLS
PLAYERSTAGE=4*CURRPLAYER+CURRSTAGE
GOSUB DRAWWALLS
GOSUB DRAWDOOR
GOSUB DRAWBOTTOM
GOSUB DRAWBRICKS
C=0
FOR B=0 TO 1
BALLS(B,0)=0
NEXT B
TM=0
SPRITE.A 2,1
SPRITE.A 3,1
LC=0
LT=0
STOP
VOLUME 2,6,
PLAY 2,26,60 SOUND 14
'...

PLACEBALL:
GOSUB CTRLBAT
SPRITE 0,12,BATY+9,C
GOSUB DRAWBAT

'GOSUB UPDATETRACE
GOSUB UPDATEDOORANIM
WAIT VBL
TICK=(TICK+1) MOD 5184000
LC=LC+1
IF TICK=6 THEN
BALL=BALL-1
PLAYERS(CURRPLAYER,0)=BALL
GOSUB DRAWLIFE
C=127
END IF
IF TOUCH THEN
CALL HIDEMSG
IF LC=40 OR LC=80 THEN
CALL SNDFX(1)
END IF
IF LC>=40 AND LC<=48 THEN
LT=70+(LC-40)\3
ELSE IF LC=49 THEN
LT=0
ELSE IF LC>=80 AND LC<=88 THEN
LT=102+(LC-80)\3
ELSE IF LC=89 THEN
LT=0
ELSE IF LC=120 THEN
CALL SNDFX(3)
CALL NEWBALL(13,BATY+10,6,2,1,1)
GOSUB MUSICCHANGE
GOTO PLAYLOOP
END IF
IF LT>0 THEN
SPRITE 2,16,48,LT
SPRITE 3,16,56,LT+16
ELSE
SPRITE OFF 2
SPRITE OFF 3
END IF
ELSE
LC=0
SPRITE OFF 2
SPRITE OFF 3
END IF
IF NOT TOUCH THEN CALL DRAWMSG
GOTO PLACEBALL

PLAYERTURN:
SPRITE OFF
COPY ROM(6),32 TO $FF00
J=CURRPLAYER
I=RND(5)+2
D=6
WHILE D<=30
CLS
BG 0
BG COPY 0,4,7,4 TO 6,6
PAL I
FOR X=6 TO 13
FOR Y=6 TO 10
CELL X,Y,
NEXT Y
NEXT X
IF J=1 THEN PAL 0
TEXT 6,2,"PLAYER 1"
S$=STR$(PLAYERS(0,1))
TEXT 9-LEN(S$)\2,3,S$
PAL I
IF J=0 THEN PAL 0
TEXT 6,12,"PLAYER 2"
S$=STR$(PLAYERS(1,1))
TEXT 9-LEN(S$)\2,13,S$
BG 1
PAL I
IF J=0 THEN
BG FILL 0,0 TO 19,7 CHAR 157
BG FILL 0,8 TO 19,8 CHAR 153
ELSE
BG FILL 0,9 TO 19,15 CHAR 157
BG FILL 0,8 TO 19,8 CHAR 158
END IF
PLAY 0,70,1 SOUND 4
WAIT D
D=D+4
J=(J+1) MOD 2
WEND
WAIT 80
PLAYERTURNSKIP:
CLS
WAIT VBL
COPY ROM(1),32 TO $FF00
RETURN

PLAYLOOP:
GOSUB UPDATEDOORANIM
GOSUB CTRLBAT
GOSUB DRAWBAT
GOSUB UPDATETRACE
GOSUB UPDATEBALL
GOSUB UPDATEANIMS
GOSUB MUSICFOLLOW
WAIT VBL
TICK=(TICK+1) MOD 5184000
GOTO PLAYLOOP

JUMPSTAGE:
N=0
FOR K=0 TO 1
IF BALLS(K,0)>=2 THEN N=-1
NEXT K
BALLTONEXT=0
IF N THEN
GOTO NEXTSTAGE
ELSE
GOTO PREVSTAGE
END IF

NEXTSTAGE:
CURRSTAGE=CURRSTAGE+1
IF CURRSTAGE=4 THEN GOTO NEXTACT
FOR B=0 TO 1
IF BALLS(B,0)<=-1 THEN CALL DELBALL(B)
BALLS(B,2)=MAX(2,BALLS(B,2)-0.5)
NEXT B
GOTO CONTINUESTAGE

PREVSTAGE:
STOP 2
STOP 3
CURRSTAGE=CURRSTAGE-1
IF CURRSTAGE=-1 THEN
GOTO BALLLOST
ELSE
CALL DELBALL((LASTBALLLOST+1) MOD 2)
PANIC=-1
COPY ROM(7),32 TO $FF00
END IF

CONTINUESTAGE:
FOR B=0 TO 1
C=BALLS(B,0)
IF C>=2 THEN SPRITE B,-3,,
IF C<=-1 THEN SPRITE B,161,, 
NEXT B
PLAYERSTAGE=4*CURRPLAYER+CURRSTAGE
IF FORCES(PLAYERSTAGE)>0 AND NOT PANIC THEN RECOVERING=0
MLC=0
GOSUB MUSICCHANGE
GOSUB DRAWWALLS
GOSUB DRAWDOOR
GOSUB DRAWBOTTOM
GOSUB DRAWBRICKS
GOTO PLAYLOOP

NEXTACT:
CURRSTAGE=0
CURRACT=CURRACT+1
STOP
SPRITE OFF 6
WAIT 40
MUSIC 48
WAIT 4
CALL GOALFX(40)
WAIT 40
MUSIC 30
CALL GOALEND
STOP
WAIT 30
GOSUB COMPUTESCORE
GOSUB DEBUILDBRICKS
BALL=BALL+1
IF CURRACT=5 THEN GOTO WINGAME
PLAYERS(CURRPLAYER,3)=CURRACT
GOSUB FILLLEVELS
GOTO STARTPLAY

DEBUILDBRICKS:
K=-1
FOR I=0 TO 153 STEP 3
BX=I\9
IF I<153 THEN
FOR J=0 TO 2
BY=(I MOD 9)+J
GOSUB BRICKTOCELL
BT=BRICKS(BX,BY,0,PLAYERSTAGE)
BH=BRICKS(BX,BY,2,PLAYERSTAGE)
IF BT>=10 AND BH>1 THEN
CT=69+R*32
CELL CX,CY,CT
CELL CX,CY+1,CT+16
END IF
NEXT J
END IF
IF K>=0 THEN
BX=K\9
FOR J=0 TO 2
BY=(K MOD 9)+J
GOSUB BRICKTOCELL
CELL CX,CY,0
CELL CX,CY+1,0
NEXT J
END IF
WAIT VBL
K=I
NEXT I
RETURN

BALLLOST:
K=RND(3)+52
CALL SNDFX(K)
FOR J=1 TO 50
GOSUB UPDATETRACE
WAIT 1
NEXT J
IF K=52 THEN WAIT 70
WAIT 70
GOSUB COMPUTESCORE
GOTO NEXTPLAYER

COMPUTESCORE:
B$=STR$(BONUS)
B=LEN(B$)
FOR I=1 TO B
V$=RIGHT$(B$,1)
B$=LEFT$(B$,B-I)
V=VAL(V$)
T=1*10^(I-1)
FOR J=1 TO V
PLAY 2,(5-I)*6+40,3+I SOUND RND(1)*5
BONUS=BONUS-T
SCORE=SCORE+T*MULTI
GOSUB DRAWSCORE
WAIT 4+I
NEXT J
WAIT 4
NEXT I
WAIT 50
PLAYERS(CURRPLAYER,1)=SCORE
RETURN

NEXTPLAYER:
IF CURRPLAYER=0 THEN POKEL $E004,SCORE
IF CURRPLAYER=1 THEN POKEL $E008,SCORE
IF SCORE>PEEKL($E000) THEN POKEL $E000,SCORE
'IF PLAYERCOUNT=1 THEN GOTO NONEXTPLAYER
CURRPLAYER=(CURRPLAYER+1) MOD PLAYERCOUNT
'CHECK LIFE NUM
IF PLAYERS(CURRPLAYER,0)=0 THEN
'CHECK OTHER PLAYER LIFE NUM
C=0
FOR P=0 TO PLAYERCOUNT-1
IF PLAYERS(P,0)>0 THEN C=-1
NEXT P
IF C THEN GOTO NEXTPLAYER
WAIT 150
GOTO ENDGAME
END IF
'GOSUB PLAYERTURN
PLAYERSTAGE=4*CURRPLAYER+CURRSTAGE
'NONEXTPLAYER:
GOSUB RESETPLAY
GOTO STARTPLAY

LOADLEVEL:
'IF FORCEFORCE>=0 THEN FORCES(PLAYERSTAGE)=FORCEFORCE
'IF FORCELEVEL>=0 THEN CURRLEVEL=FORCELEVEL
A=CURRLEVEL*6+1+ROM(4)
LEVELFILE=PEEK(A)
LEVELADDR=PEEK(A+1)*255+PEEK(A+2)
LEVELSIZE=PEEK(A+3)*255+PEEK(A+4)
C=PEEK(A+5)
A=LEVELADDR+ROM(LEVELFILE)
FOR G=0 TO 6
R=PEEK(A)
IF R=0 THEN
V=99
ELSE IF R=1 THEN
V=2
ELSE IF R=2 THEN
V=5
ELSE IF R=3 THEN
V=9
END IF
GROUPS(G,2,PLAYERSTAGE)=V
A=A+1
NEXT G
FOR X=16 TO 16-C+1 STEP -1
FOR Y=0 TO 8
F=PEEK(A)
S=PEEK(A+1)
T=F AND %00001111
IF T=2 OR T=8 OR T>=10 THEN
W=2
ELSE IF T=5 THEN
W=6
ELSE
W=1
END IF
G=((S AND %00111000)/8)-1
H=-(F AND %10000000)/128
BRICKS(X,Y,0,PLAYERSTAGE)=T
BRICKS(X,Y,1,PLAYERSTAGE)=(F AND %01110000)/16
IF H THEN
BRICKS(X,Y,2,PLAYERSTAGE)=0
ELSE
BRICKS(X,Y,2,PLAYERSTAGE)=W
END IF
BRICKS(X,Y,3,PLAYERSTAGE)=S AND %111
BRICKS(X,Y,4,PLAYERSTAGE)=G+1
BRICKS(X,Y,7,PLAYERSTAGE)=W
BRICKS(X,Y,8,PLAYERSTAGE)=0
IF G>=0 THEN
IF NOT H THEN
GROUPS(G,3,PLAYERSTAGE)=GROUPS(G,3,PLAYERSTAGE)+1
GROUPS(G,4,PLAYERSTAGE)=GROUPS(G,4,PLAYERSTAGE)+1
END IF
IF GROUPS(G,0,PLAYERSTAGE)>=0 THEN
BRICKS(X,Y,5,PLAYERSTAGE)=GROUPS(G,0,PLAYERSTAGE)
BRICKS(X,Y,6,PLAYERSTAGE)=GROUPS(G,1,PLAYERSTAGE)
ELSE
BRICKS(X,Y,5,PLAYERSTAGE)=-1
END IF
GROUPS(G,0,PLAYERSTAGE)=X
GROUPS(G,1,PLAYERSTAGE)=Y
END IF
A=A+2
NEXT Y
NEXT X
RETURN

RESETLEVEL:
'RESET GROUPS
FOR I=0 TO 6
GROUPS(I,2,PLAYERSTAGE)=0
GROUPS(I,0,PLAYERSTAGE)=-1
GROUPS(I,3,PLAYERSTAGE)=0
GROUPS(I,4,PLAYERSTAGE)=0
NEXT I
'RESET BRICKS
FOR X=16 TO 0 STEP -1
FOR Y=0 TO 8
BRICKS(X,Y,0,PLAYERSTAGE)=0
BRICKS(X,Y,1,PLAYERSTAGE)=0
BRICKS(X,Y,2,PLAYERSTAGE)=0
BRICKS(X,Y,3,PLAYERSTAGE)=0
BRICKS(X,Y,4,PLAYERSTAGE)=0
BRICKS(X,Y,5,PLAYERSTAGE)=0
BRICKS(X,Y,6,PLAYERSTAGE)=0
BRICKS(X,Y,7,PLAYERSTAGE)=0
BRICKS(X,Y,8,PLAYERSTAGE)=0
NEXT Y
NEXT X
FORCES(PLAYERSTAGE)=7
RETURN

RESETPLAY:
BALL=PLAYERS(CURRPLAYER,0)
SCORE=PLAYERS(CURRPLAYER,1)
LETTERS=PLAYERS(CURRPLAYER,2)
CURRACT=PLAYERS(CURRPLAYER,3)
CURRSTAGE=0
'IF FORCECURRSTAGE>=0 THEN CURRSTAGE=FORCECURRSTAGE
BONUS=0
MULTI=1
DOORWASCLOSED=0
BALLTONEXT=0
LASTBALLLOST=-1
FOR B=0 TO 1
BALLS(B,0)=0
NEXT B
PANIC=0
AUTO=0
AUTOY=0
RECOVERING=0
COPY ROM(1),32 TO $FF00
'GOSUB MUSICCHANGE
RETURN

'==================================================================

WINGAME:
FOR II=1 TO 50
GOSUB UPDATETRACE
WAIT VBL
NEXT II
'ADD BALL TO BONUS
FOR II=BALL TO 1 STEP -1
BALL=BALL-1
SCORE=SCORE+333
GOSUB DRAWLIFE
GOSUB DRAWSCORE
WAIT 50
NEXT II
PLAYERS(CURRPLAYER,0)=0
WAIT 50
GOSUB COMPUTESCORE
GOTO NEXTPLAYER

'==================================================================

BRICKTOCELL:
R=BY MOD 2
CX=BX+2
CY=BY*2-(BY\2)
BG R
RETURN

DRAWBRICKS:
FOR BX=16 TO 0 STEP -1
FOR BY=0 TO 8
GOSUB DRAWBRICK
NEXT BY
NEXT BX
RETURN

DRAWBRICK:
ATTR BRICKS(BX,BY,1,PLAYERSTAGE)+1
GOSUB BRICKTOCELL
BT=BRICKS(BX,BY,0,PLAYERSTAGE)
BH=BRICKS(BX,BY,2,PLAYERSTAGE)
IF BT>=10 AND BH>1 THEN
CT=1+R*32
ELSE IF BT=8 THEN
IF BH=1 THEN CT=1+R*32 ELSE CT=0
ELSE
IF BT>=10 THEN ATTR 1

IF BT=15 THEN BT=64+PLAYERS(CURRPLAYER,4)

IF BH>0 THEN
CT=BT+R*32
