CELL CX,CY+1,CT+16
END IF

ELSE
BX=ANIMS(I,0)
BY=ANIMS(I,1)
ANIMS(I,2)=0

IF A=1 OR A=2 OR A=3 THEN
GOSUB DRAWBRICK
END IF

END IF
END IF

NEXT I
RETURN

UPDATEDOORANIM:
IF NOT PANIC AND TIMER MOD 3=0 THEN
BG 1
ATTR 1
FOR Y=0 TO 13
T=CELL.C(19,Y)
IF T=90 OR T=106 OR T=91 OR T=107 THEN
CELL 19,Y,T+1
ELSE
T=126
IF FORCES(PLAYERSTAGE)=0 THEN T=125-(TIMER MOD 4)
CELL 19,Y,T
END IF
NEXT Y
ELSE IF PANIC THEN
BG 1
ATTR 1
FOR Y=0 TO 13
CELL 19,Y,77
NEXT Y
END IF
RETURN

'==================================================================

INPUTNAME:
CLS
BG 0
INPUTLOOP:
WAIT VBL
GOTO INPUTLOOP

TITLERANDOMCOL:
C=INT(RND*4)*16+INT(RND*4)*4+INT(RND*4)
IF RND>0.71 THEN
'PALETTE TITLEPAL,0,63,C,C AND %110110110
PALETTE TITLEPAL,0,C,C AND %011011011,C AND %110110110
ELSE
PALETTE TITLEPAL,0,0,C,C
END IF
RETURN

TITLESCREEN:
WAIT VBL
CLS
SPRITE OFF
ON VBL OFF
COPY ROM(6),32 TO $FF00
SCROLL 0,-4,0

IF PLAYERCOUNT=1 THEN
BG COPY 14,0,6,2 TO 6,1
BG COPY 0,0,14,2 TO 2,4
BG COPY 0,2,16,2 TO 2,6
PAL 0
TEXT 4,9,"TAP TO PLAY"
BG FILL 4,10 TO 14,10 CHAR 105
TEXT 3,11,"LAST"
S$=STR$(PEEKL($E004))
TEXT 3,12,S$
TEXT 12,11,"HIGH"
S$=STR$(PEEKL($E000))
TEXT 16-LEN(S$),12,S$
TEXT 1,14,"SETTINGS"
TEXT 13,14,"INFOS"
BG FILL 1,15 TO 8,15 CHAR 105
BG FILL 13,15 TO 17,15 CHAR 105

TITLEPAL=7
PALETTE TITLEPAL,0,0,0,0

TITLESCREENSWIPE:
TITLEX=2
TITLEY=0
TITLEPAL=((TITLEPAL+1) MOD 2)+6
GOSUB TITLERANDOMCOL
ATTR TITLEPAL
ON VBL CALL TITLEINTER

FOR I=0 TO 100

X=TOUCH.X\8
Y=TOUCH.Y\8
IF TAP AND Y=14 AND X>=1 AND X<=9 THEN GOTO SETTINGPAGE
IF TAP AND Y=14 AND X>=13 AND X<=18 THEN GOTO INFOPAGE
IF TAP AND X>=2 AND Y>=7 AND X<=17 AND Y<=11 THEN GOTO NEWGAME

WAIT VBL
NEXT I
GOTO TITLESCREENSWIPE

ELSE IF PLAYERCOUNT=2 THEN

BG 1
TITLEPAL=RND(5)+2
PAL TITLEPAL
BG FILL 0,0 TO 19,7 CHAR 157
BG FILL 0,8 TO 19,8 CHAR 153

BG 0

S$=NAMES$(0)
TEXT 10-LEN(S$)/2,3,S$
S$=STR$(PEEKL($E004))
TEXT 9-LEN(S$)\2,4,S$

PAL 0
S$=NAMES$(1)
TEXT 10-LEN(S$)/2,11,S$
S$=STR$(PEEKL($E008))
TEXT 9-LEN(S$)\2,12,S$

BG COPY 0,4,7,4 TO 6,6
PAL RND(5)+2
FOR X=6 TO 13
FOR Y=6 TO 10
CELL X,Y,
NEXT Y
NEXT X

DO
X=TOUCH.X\8
Y=TOUCH.Y\8
IF TAP AND X>=6 AND Y>=7 AND X<=12 AND Y<=9 THEN GOTO NEWGAME
WAIT VBL
LOOP
END IF

SUB TITLEINTER
CELL TITLEX,TITLEY+4,
TITLEY=(TITLEY+1) MOD 4
IF TITLEY=0 THEN TITLEX=TITLEX+1
IF TITLEX=18 THEN ON VBL OFF
END SUB

SUB SNDFX(T)
TRACK T,SFX
SFX=(SFX+1) MOD 2
END SUB

INFOPAGE:
SPRITE OFF
ON VBL OFF
WAIT VBL
CLS
COPY ROM(1),32 TO $FF00
BG 1
PAL 0
BG FILL 0,0 TO 19,15 CHAR 121
BG SOURCE ROM(10)
BG 0
BG COPY 0,0,20,16 TO 0,0
SCROLL 0,4,4
DO
WAIT VBL
IF TAP THEN GOTO THXPAGE
LOOP

THXPAGE:
BG COPY 0,32+16,20,16 TO 0,0
BG SOURCE ROM(3)
DO
WAIT VBL
IF TAP THEN GOTO TITLESCREEN
LOOP

SETTINGPAGE:
SPRITE OFF
ON VBL OFF
WAIT VBL
CLS
COPY ROM(1),32 TO $FF00
BG 1
PAL 0
BG SOURCE ROM(10)
BG 1
BG COPY 0,32,20,16 TO 0,0
BG 0
BG COPY 0,16,20,16 TO 0,0
BG SOURCE ROM(3)

PAL 3
TEXT 5,6,CTRNAME$(PLAYERS(0,5))
TEXT 5,8,CTRNAME$(PLAYERS(1,5))

DO
WAIT VBL
X=TOUCH.X\8
Y=TOUCH.Y\8
PAL 3
IF TAP AND Y=2 AND X>=5 AND X<=14 THEN PLAYERCOUNT=(PLAYERCOUNT MOD 2)+1
IF TAP AND Y=6 AND X>=5 AND X<=11 THEN CALL INCRPLAYERCTR(PLAYERS(0,5))
IF TAP AND Y=8 AND X>=5 AND X<=11 THEN CALL INCRPLAYERCTR(PLAYERS(1,5))
IF TAP AND Y=14 AND X>=1 AND X<=4 THEN GOTO TITLESCREEN

IF TAP THEN
TEXT 5,2,"ONE"
IF PLAYERCOUNT=2 THEN TEXT 5,2,"TWO"
TEXT 5,6,CTRNAME$(PLAYERS(0,5))+"   "
TEXT 5,8,CTRNAME$(PLAYERS(1,5))+"   "
POKE $E00C,PLAYERS(0,5)+1
POKE $E00D,PLAYERS(1,5)+1
END IF

IF TAP AND Y=12 THEN
IF X>=5 AND X<=6 THEN MUSIC 1
IF X>=9 AND X<=10 THEN MUSIC 9
IF X>=13 AND X<=14 THEN MUSIC 17
END IF

LOOP

SUB INCRPLAYERCTR(C)
C=(C+1) MOD 5
END SUB

