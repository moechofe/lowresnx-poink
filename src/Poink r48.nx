END IF

ELSE IF TARGETX>8 AND TARGETX<11 AND AH<0 AND 999=666 THEN
AH=1
BALLS(B,1)=0
IF E>=-10 AND E<0 THEN
AV=1
ELSE IF E>=0 AND E<10 THEN
AV=-1
