SUB USERCTR(C,Y)
IF C=0 THEN
Y=TOUCH.Y
ELSE IF C=1 THEN
Y=((TOUCH.Y-58)*1.5)+58
ELSE IF C=2 THEN
Y=((TOUCH.Y-58)*2.5)+58
ELSE IF C=3 THEN
Y=((TOUCH.Y-58)*3.5)+58
ELSE IF C=4 THEN
Y=((TOUCH.Y-58)*4.5)+58
END IF
END SUB

