MUSICSTART:
DATA 1,9,17,38,46
DATA 23,31,32,33,34,35,36,38,46

MUSICCHANGE:
IF FORCES(PLAYERSTAGE)>0 THEN
R=RND*13
IF CURRSTAGE=0 THEN R=RND*5
RESTORE MUSICSTART
REPEAT
READ MSC
DEC R
UNTIL R<=0
CURRPATTERN=MSC
MLC=0
END IF
RETURN

MUSICFOLLOW:
IF NOT PANIC AND FORCES(PLAYERSTAGE)>0 AND NOT RECOVERING THEN
IF MLC MOD 2048=0 THEN GOSUB MUSICCHANGE
IF MLC MOD 1024=0 OR (MLC MOD 512=0 AND RND<0.5) THEN
R=INT(RND*2)
IF R=0 THEN
MAC=(MBC+1+INT(RND*3)) MOD 4
ELSE
MBC=(MAC+1+INT(RND*3)) MOD 4
END IF
END IF
IF MLC MOD 256=0 THEN
A=ROM(15)+128+4*CURRPATTERN
TRACK PEEK(A+MAC) AND %01111111,2
TRACK PEEK(A+MBC) AND %01111111,3
IF (PEEK(A+0) AND %10000000)>0 THEN INITPATTERN=CURRPATTERN
IF (PEEK(A+1) AND %10000000)>0 THEN
CURRPATTERN=INITPATTERN
ELSE
CURRPATTERN=CURRPATTERN+1
END IF
END IF
END IF
MLC=MLC+1
RETURN

'COPY SOME CHARA FROM A ROM TO THE MEMORY AT $8000
'(IN) A BASE ADDRESS FOR THE CHARA DATA
'(IN) S SOURCE CHARA NUM
'(IN) C CHARA COUNT TO COPY
'(IN) D DEST CHARA INDEX
SUB COPYCHARA(A,S,C,D)
'IN A BASE ADDRESS
'IN S SOURCE CHARA INDEX
'IN C CHARA COUNT TO COPY
'IN D DEST CHARA INDEX
COPY A+S*16,C*16 TO $8000+D*16
END SUB

SUB GOALFX(D)
ON VBL CALL GOALVBL
FOR I=7 TO 63
SPRITE I,RND*160-7,RND*128-7,70+RND*3
SPRITE I PAL 2+RND*6
NEXT I
END SUB

SUB GOALEND
FOR J=0 TO 140
FOR I=7 TO 63
IF SPRITE.Y(I)>=127 THEN
SPRITE I,0,0,0
SPRITE I PAL 1
END IF
NEXT I
WAIT VBL
NEXT J
ON VBL OFF
WAIT VBL
CALL COPYCHARA(ROM(3),70,2,70)
END SUB

SUB GOALVBL
T=TIMER\6
CALL COPYCHARA(ROM(11),T MOD 12,1,70)
CALL COPYCHARA(ROM(11),(T+3) MOD 12,1,71)
CALL COPYCHARA(ROM(11),(T+7) MOD 12,1,72)
FOR I=7 TO 63
SPRITE I,,((SPRITE.Y(I)+8) MOD 136)-7,
NEXT I
END SUB

